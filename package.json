{
  "version": "1.0.0",
  "scripts": {
    "build": "webpack $npm_package_config_production",
    "type-check-then-lint-staged": "npm run type-check && lint-staged",
    "serve": "PORT=$npm_package_config_development_server_port webpack-dev-server $npm_package_config_development",
    "test": "echo 'Awesome!'",
    "type-check": "tsc --project ./src/tsconfig.json"
  },
  "config": {
    "development_server_port": 10000,
    "development": "--config ./build/development.configuration.babel.js",
    "production": "--config ./build/production.configuration.babel.js"
  },
  "dependencies": {
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^1.19.1",
    "typescript": "^3.8.2",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "license": "MIT"
}
