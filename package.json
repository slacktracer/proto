{
  "version": "1.0.0",
  "scripts": {
    "build": "webpack $npm_package_config_production",
    "deploy": "s3-deploy './dist/**' --cwd './dist/' --region us-east-1 --bucket proto.slacktracer.com --distId E30FFC1RICEDL3 --invalidate 'index.html' --deleteRemoved",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "serve": "PORT=$npm_package_config_development_server_port webpack-dev-server $npm_package_config_development",
    "test": "jest",
    "type-check": "tsc --project ./src/tsconfig.json",
    "type-check-then-lint-staged": "npm run type-check && lint-staged"
  },
  "config": {
    "development_server_port": 10000,
    "development": "--config ./build/development.configuration.babel.js",
    "production": "--config ./build/production.configuration.babel.js"
  },
  "dependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.2.3",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.2",
    "s3-deploy": "^1.4.0",
    "typescript": "^3.8.2",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.0",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "webpack-dev-server": "^3.10.3"
  },
  "license": "MIT"
}
